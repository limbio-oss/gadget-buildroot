#
# Automatically generated file; DO NOT EDIT.
# Swupdate Configuration
#
CONFIG_HAVE_DOT_CONFIG=y

#
# Swupdate Settings
#

#
# General Configuration
#
CONFIG_SCRIPTS=y
CONFIG_HW_COMPATIBILITY=y
CONFIG_HW_COMPATIBILITY_FILE="/etc/hwrevision"
CONFIG_SW_VERSIONS_FILE="/etc/sw-versions"
CONFIG_MTD=y

#
# Lua support needs a Lua interpreter
#
# CONFIG_FEATURE_SYSLOG is not set

#
# Build Options
#
# CONFIG_STATIC is not set
CONFIG_CROSS_COMPILE="/opt/output/host/usr/bin/arm-linux-gnueabihf-"
CONFIG_SYSROOT="/opt/output/host/usr/arm-buildroot-linux-gnueabihf/sysroot"
CONFIG_EXTRA_CFLAGS="-D_LARGEFILE_SOURCE -D_LARGEFILE64_SOURCE -D_FILE_OFFSET_BITS=64  -Os "
CONFIG_EXTRA_LDFLAGS=""
CONFIG_EXTRA_LDLIBS=""

#
# Debugging Options
#
# CONFIG_DEBUG is not set
# CONFIG_WERROR is not set
# CONFIG_NOCLEANUP is not set

#
# Image downloading support needs libcurl
#
CONFIG_SIGNED_IMAGES=y
# CONFIG_SURICATTA is not set
# CONFIG_WEBSERVER is not set

#
# Archival Features
#
CONFIG_GUNZIP=y

#
# Parser Features
#
CONFIG_LIBCONFIG=y
CONFIG_LIBCONFIGROOT=""

#
# JSON config parser support needs json-c
#
# CONFIG_SETSWDESCRIPTION is not set

#
# Image Handlers
#
CONFIG_UBIVOL=y
CONFIG_UBIBLACKLIST=""
CONFIG_UBIVIDOFFSET=0
# CONFIG_CFI is not set
# CONFIG_CFIHAMMING1 is not set
CONFIG_RAW=y
CONFIG_SHELLSCRIPTHANDLER=y

#
# archive support needs libarchive
#
CONFIG_UBOOT=y
CONFIG_UBOOT_FWENV="/etc/fw_env.config"
